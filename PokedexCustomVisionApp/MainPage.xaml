             Padding="0,60,0,0"
            xmlns:base="clr-namespace:Xamarin.Forms.MVVMBase.Page;assembly=Xamarin.Forms.MVVMBase"
             mc:Ignorable="d" x:Class="PokedexCustomVisionApp.MainPage">
    <StackLayout>
        <!-- Place new controls here -->
        <Label Text="Bem Vindo a Pokedex"  />
